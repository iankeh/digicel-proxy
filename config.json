            {
              "certificateFile": "/app/cert.pem",
              "keyFile": "/app/key.pem"
            }
          ],
          "alpn": ["h2", "http/1.1"]
        },
        "httpSettings": {
          "host": [
            "mda.digicelgroup.com",
            "chat.digicelpacific.com",
            "digicelid.digicelpacific.com",
            "mydigicelpng.digicelpacific.com"
          ],
          "path": "/api/v1/stream"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
